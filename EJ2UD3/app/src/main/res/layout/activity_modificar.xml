<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="10dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_nombre"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginVertical="10dp"
                android:text="Nombre: "/>

            <EditText
                android:layout_below="@id/tv_nombre"
                android:id="@+id/et_nombre"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

            <TextView
                android:layout_below="@id/et_nombre"
                android:id="@+id/tv_apellidos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginVertical="10dp"
                android:text="Apellidos: "/>

            <EditText
                android:layout_below="@id/tv_apellidos"
                android:id="@+id/et_apellidos"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

            <TextView
                android:id="@+id/tv_fecha"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/et_apellidos"
                android:text="Fecha: "
                android:textColor="#000000"
                android:layout_marginTop="10dp"
                android:textSize="20dp" />

            <EditText
                android:id="@+id/et_fecha"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:hint="dd mm yyyy"
                android:textAlignment="center"
                android:layout_alignParentRight="true"
                android:layout_below="@id/et_apellidos"/>

            <TextView
                android:id="@+id/tv_genero"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/et_fecha"
                android:text="Genero: "
                android:textColor="#000000"
                android:textSize="20dp"
                android:layout_marginVertical="10dp"/>

            <Spinner
                android:id="@+id/s_genero"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:hint="dd mm yyyy"
                android:textAlignment="center"
                android:layout_alignParentRight="true"
                android:layout_below="@id/et_fecha"
                android:layout_marginVertical="10dp"/>

            <TextView
                android:id="@+id/tv_prov"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/s_genero"
                android:layout_marginTop="15dp"
                android:text="Provincia:"
                android:textColor="#000000"
                android:textSize="20dp"
                android:layout_marginVertical="10dp"/>

            <Spinner
                android:id="@+id/s_provincia"
                android:layout_width="180dp"
                android:layout_height="wrap_content"
                android:textSize="20dp"
                android:layout_marginTop="15dp"
                android:hint="dd mm yyyy"
                android:textAlignment="center"
                android:layout_alignParentRight="true"
                android:layout_below="@id/s_genero"
                android:layout_marginVertical="10dp"/>

            <CheckBox
                android:layout_below="@id/s_provincia"

                android:text="Consiente el uso de datos"
                android:id="@+id/cb_consentimiento"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

            <Button
                android:layout_below="@id/cb_consentimiento"
                android:id="@+id/bt_aceptar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="10dp"
                android:onClick="guardar"
                android:text="guardar"/>
        </RelativeLayout>
    </ScrollView>
